{
	"manifest_version": 2,
	"name": "oof",
	"version": "0.0.2",
	"description": "You die every time you click a link or button",
	"content_scripts": [
	    {
	      "matches": ["*://*/*"],
          "js": ["main.js"],
          "run_at": "document_end"
	    }
      ],
      "web_accessible_resources": [
        "audio/*",
        "audio/oof.mp3"
    ]
}